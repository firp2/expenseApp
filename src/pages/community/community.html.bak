<ion-header>
  <ion-navbar>
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    <ion-title>
      community
    </ion-title>
    <ion-buttons end>
        <ion-icon name="camera"></ion-icon>
    </ion-buttons>
  </ion-navbar>
</ion-header>
<ion-content padding id="page4">
  <ion-card id="community-card23" *ngFor = "let communityItem of community" >
    <ion-list>
      <ion-item color="none" id="community-list-item100">
        <ion-avatar item-left>
          <img src="" />
        </ion-avatar>
        <h2>
          {{communityItem.username}}
        </h2>
      </ion-item>
      <img src="{{communityItem.picture}}" on-click="goToPhotoIntroduce(communityItem)"/>
      <ion-item color="none" id="community-list-item101">
        {{communityItem.notes}}
        <ion-icon name="{{icon}}" item-right (click) = "changeColor()" color="{{color}}"></ion-icon>
      
      </ion-item>
    </ion-list>
  </ion-card>
  </ion-content>
     
  
  
  
  
  
  
